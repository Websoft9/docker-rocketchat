{
  "name": "rocketchat",
  "trademark": "RocketChat",
  "installpath": "/data/wwwroot/rocketchat",
  "compose_file": "docker-compose.yml",
  "fork_url": "https://github.com/RocketChat/Docker.Official.Image",
  "env_file": ".env",
  "requirements":{
    "cpu_arch": "Linux x86-64, ARM 32/64, Windows x86-64, IBM POWER8, x86/i686",
    "cpu": "1",
    "memory": "2",
    "disk": "20",
    "swap": "2",
    "url": "https://docs.rocket.chat/installing-and-updating/docker-containers"
  },

  "services":[
    {
      "name": "Rocket.Chat",
      "port": "9001",
      "description_cn": "浏览器访问 Rocket.Chat",
      "description_en": "HTTP to access Rocket.Chat",
      "required": "Y"
    }
  ]

}
